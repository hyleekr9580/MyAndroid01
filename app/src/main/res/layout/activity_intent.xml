<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="kr.contentsstudio.myfirstandroidapp.IntentActivity">

    <Button
        android:id="@+id/call_btn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="전화걸기" />
    <Button
        android:id="@+id/actin_call_btn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="전화직접걸기" />

    <Button
        android:id="@+id/web_btn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="웹페이지열기" />

    <Button
        android:id="@+id/camera_btn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="카메라 video mode" />

    <Button
        android:id="@+id/stil_btn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="사진 still image mode" />

    <Button
        android:id="@+id/pic_btn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="사진첩" />
</LinearLayout>
